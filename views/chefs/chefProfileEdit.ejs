<form method="POST" action="/chefs/<%=adminChefItem._id%>?_method=PUT">
  <h2>Add more info</h2>
  <div class="form-group row">
    <label class="form-control" for="name">Name</label>
    <input type="text" class="form-control" name="chefs[name]" value="<%=adminChefItem.local.name%>" id="name" placeholder="name">
  </div>

  <div class="form-group row">
    <label class="form-control" for="Info">Info</label>
    <input type="text" class="form-control" name="chefs[intro]" value="<%=adminChefItem.local.intro%>" id="intro" placeholder="Introduction">
  </div>

  <div class="form-group row">
    <label class="form-control" for="Address">Address</label>
    <input type="text" class="form-control" name="chefs[Address]" value="<%=adminChefItem.local.Address%>" id="Address" placeholder="Address">
  </div>

  <div class="form-group row">
    <label class="form-control" for="cuisines">Cuisines</label>
    <input type="text" class="form-control" name="chefs[cuisines]" value="<%=adminChefItem.local.cuisines%>" id="cuisines" placeholder="cuisines">
  </div>

  <button type="submit" class="btn btn-success">Save</button>
  </form>
