<div class="chef-login">
<h1>Hi Chef! Welcome back to Kitchen Wonders!</h1>
<img class="chef-image" src="/images/chef-image.png" alt="chef-pic">
</div>
<h3>Please login to proceed</h3>
<% include ../partials/flash %>
<form action="/chefs/logged-chef" method="post">
  <div class="login-chef-form">
    <div class="form-group">
      <label for="email">Email address</label>
      <input type="email" class="form-control" name="email" id="email" placeholder="email">
    </div>
    <div class="form-group">
      <label for="password">Password</label>
      <input type="password" class="form-control" name="password" id="password" placeholder="password">
    </div>
    <div class="form-group">
      <button class="btn btn-primary" type="submit">Submit</button>
    </div>
  </div>
</form>
