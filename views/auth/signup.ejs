<div class="signin-customer">
<h1>Welcome to Kitchen Wonders!</h1>
<h3>Please sign up to proceed</h3>
</div>
<%# <h4>Which account do you want to create?</h4>%>
<% include ../partials/flash %>
<form action="/signup" method="post">
  <div class="signin-customer-form">
  <div class="form-group">
    <label for="firstName">First Name</label>
    <input type="text" class="form-control" name="firstName" id="firstName" placeholder="firstName">
  </div>
  <div class="form-group">
    <label for="LastName">Second Name</label>
    <input type="text" class="form-control" name="LastName" id="LastName" placeholder="LastName">
  </div>
  <div class="form-group">
    <label for="Address">Address</label>
    <input type="text" class="form-control" name="Address" id="Address" placeholder="Address">
  </div>
  <div class="form-group">
    <label for="postal code">Postal Code</label>
    <input type="text" class="form-control" name="postalCode" id="postalCode" placeholder="postalCode">
  </div>
  <div class="form-group">
    <label for="email">Email address</label>
    <input type="email" class="form-control" name="email" id="email" placeholder="email required">
  </div>
  <div class="form-group">
    <label for="password">Password</label>
    <input type="password" class="form-control" name="password" id="password" placeholder="password required">
  </div>
  <div class="form-group">
<button class="btn btn-primary" type="submit">Submit</button>
</div>
</div>
</form>
